{
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "directories-bin": [
        "config"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor"
        },
        {
            "in": "src/Spark/Plugins",
            "ignoreVCS": false,
            "ignoreDotFiles": false
        }
    ],
    "git-version": "package_version_spark",
    "main": "app/console",
    "output": "spark.phar",
    "stub": true
}